{
    "name": "hkonnet/laravel-ebay",
    "description": "This package is wrapper for php Ebay sdk for laravel to automate all the configurations and make the skd ready to use.",
    "type": "library",
    "keywords": ["ebay", "api", "sdk", "laravel", "laravel5", "php"],
    "license": "MIT",
    "repositories": [ {
        "type": "vcs",
        "url": "https://github.com/adetamore/ebay-sdk-php"
    }],
    "require": {
        "php": ">=5.5",
        "dts/ebay-sdk-php": "dev-fix"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Haroon Khan",
            "email": "hkonnet@gmail.com"
        }
    ],
    "autoload":{
        "psr-4": {
            "Hkonnet\\LaravelEbay\\": "src"
        }
    },
    "minimum-stability": "stable"
}
